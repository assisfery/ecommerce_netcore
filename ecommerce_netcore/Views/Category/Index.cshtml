@*
    
*@
@{
    ViewData["Title"] = "Category";
}
<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Order</th>
                <th>Created At</th>
                <th>Updated At</th>
            </tr>
        </thead>
        <tbody>
            @foreach(Category obj in Model)
            {
                <tr>
                    <td>
                        @obj.Name
                    </td>
                     <td>
                        @obj.DisplayOrder
                    </td>
                     <td>
                        @obj.CreatedDateTime
                    </td>
                     <td>
                        @obj.UpdatedDateTime
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
